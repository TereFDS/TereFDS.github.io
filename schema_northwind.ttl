@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix nwd: <http://northwind.com.ar/db/>.
@prefix foaf: < http://xmlns.com/foaf/spec/>.
@prefix dc: <http://purl.org/dc/elements/1.1/>. 

<#orders_Northwind>
	a rr:TripleMap;
	rr:tableName “Orders”;
	rr:subjectMap [
		Rr:class nwd:Order;
		Rr:template “http://northwind.com.ar/db/order/{OrderID}”;
	];
	rr:predicateObjectMap [
		rr:predicate nwd:orderid;
		rr:objectMap [
			rr:column “OrderId”;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:hasCustomer;
		rr:refObjectMap [
			rr:parentTriplesMap <#customer_Northwind>;
			rr:joinCondition [
				rr:parent “CustomerID”;
				rr:child “CustomerID”;
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:hasEmployee;
		rr:refObjectMap [
			rr:parentTriplesMap <#employee_Northwind>;
			rr:joinCondition [
				rr:parent “EmployeeID”;
				rr:child “EmployeeID”;
			];
		];
	];

	rr:predicateObjectMap [
		rr:predicate nwd:orderdate;
		rr:objectMap [
			rr:column “OrderDate”;
			rr:datatype xsd:dateTime;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:requireddate;
		rr:objectMap [
			rr:column “RequiredDate”;
			rr:datatype xsd:dateTime;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:shippeddate;
		rr:objectMap [
			rr:column “ShippedDate”;
			rr:datatype xsd:dateTime;
		];
	];
	
	rr:predicateObjectMap [
		rr:predicate nwd:shipvia;
		rr:refObjectMap [
			rr:parentTriplesMap <#shippers_Northwind>;
			rr:joinCondition [
				rr:parent “ShipID”;
				rr:child “ShipVia”;
			];
		];
	];

	rr:predicateObjectMap [
		rr:predicate foaf:name;
		rr:objectMap [
			rr:column “ShipName”;
			rr:datatype xsd:string;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:address;
		rr:objectMap [
			rr:column “ShipAddress”;
			rr:datatype xsd:string;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:city;
		rr:objectMap [
			rr:column “ShipCity”;
			rr:datatype xsd:string;
		];
];
rr:predicateObjectMap [
		rr:predicate nwd:region;
		rr:objectMap [
			rr:column “ShipRegion”;
			rr:datatype xsd:string;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:postalcode;
		rr:objectMap [
			rr:column “ShipPostalCode”;
			rr:datatype xsd:string;
		];
	];
	rr:predicateObjectMap [
		rr:predicate nwd:country;
		rr:objectMap [
			rr:column “ShipCountry”;
			rr:datatype xsd:string;
		];
	];

